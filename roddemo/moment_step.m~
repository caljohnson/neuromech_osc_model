function [ output_args ] = moment_step( X,c,Ns,no_mod_points, no_mods, S)
%MOMENT_STEP Creates the driving moment to the beam
%   

%spreads output of each moment to section of beam
driving_moment = zeros(Ns,1);
for jj=0:no_mods-1 
  driving_moment(1+jj*no_mod_points:(jj+1)*no_mod_points) = ...
      c*(tanh(S*X(3+jj*4)-2) - tanh(S*X(4+jj*4)-2));
end

c*(tanh(X(4)-2) - tanh(X(5) - 2))

end

